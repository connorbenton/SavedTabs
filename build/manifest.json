{
    "icons": {
        "96": "save_48.png"
    },
    "manifest_version": 2,
    "name": "SavedTabs",
    "description": "Saves your tabs and displays them on your New Page.",
    "version": "3",
    "author": "Connor Benton",
    "permissions": [
        "activeTab",
        "tabs",
        "topSites",
        "http://*/*",
        "https://*/*",
        "chrome://favicon/*",
        "storage"
    ],
   "chrome_url_overrides" : {
     "newtab": "newtab.html"
   }
}
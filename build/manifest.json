{
    "manifest_version": 2,
    "name": "SavedTabs",
    "description": "Saves your tabs and displays them on your New Page.",
    "version": "1",
    "author": "Connor Benton",
    "permissions": [
        "activeTab",
        "tabs",
        "topSites",
        "http://*/*",
        "https://*/*",
        "chrome://favicon/*",
        "storage"
    ],
   "browser_action": {
      "default_title": "Have a good day"
    },
   "chrome_url_overrides" : {
     "newtab": "newtab.html"
   },
   "content_scripts": [{
       "matches": ["<all_urls>"],
       "js": ["newtab.js"]
   }]
}